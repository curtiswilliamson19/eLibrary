<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
				   assembly="LibraryManagementApplication"
				   namespace="LibraryManagementApplication.Model">
	<class name="Member" table="Member">
		<id name ="ID" column="ID" type="System.Guid">
			<generator class="guid.comb"/>
		</id>
		<property name="FirstName" column="FirstName" type="System.String" not-null="true"/>
		<property name="LastName" column="LastName" type="System.String" not-null="true"/>
		<property name="EmailAddress" column="EmailAddress" type="System.String" not-null="true"/>
		<property name="PhoneNumber" column="PhoneNumber" type="System.String" not-null="true"/>
	</class>
</hibernate-mapping>

